<?xml version="1.0" encoding="utf-8"?>
<!-- Java Webstart JNLP Velocity Template for use with Webstart Maven Plugin -->
<jnlp
        spec="1.0+"
        codebase="$$codebase"
        context="$$context"
        href="$$name">
    <information>
        <title>Renjin Webstart Integration Test</title>
        <vendor>BeDataDriven</vendor>
        <description>Test of Renjin in Webstart Environment</description>
        <offline-allowed/>
    </information>
    <security>
        <all-permissions/>
    </security>
    <resources>
        <j2se version="1.7+" /> <!-- define required Java runtime version -->
        $dependencies
    </resources>
    <application-desc main-class="$mainClass"/>
</jnlp>